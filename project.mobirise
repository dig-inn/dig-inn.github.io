"buttonColor\" value=\"#d22948\">\n        <input type=\"range\" inline=\"\" title=\"Carousel Button Size\" name=\"buttonSize\" min=\"8\" max=\"20\" step=\"1\" value=\"11\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#2f2447\" condition=\"overlay\">\n        <input type=\"range\" inline=\"\" title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n    <div class=\"container-fluid\">\n        <div class=\"slider-box\" data-autoplay=\"{{autoplay}}\" data-delay=\"{{delay}}\"><amp-carousel class=\"carousel\" height=\"9\" width=\"16\" layout=\"responsive\" type=\"slides\" loop=\"\"><div class=\"carousel-img mobirise-loader mbr-flex mbr-jc-c\">\n                    <div class=\"mbr-overlay\" mbr-if=\"overlay\"></div>\n                    <amp-img src=\"@PROJECT_PATH@/assets/images/mbr.jpg\" height=\"9\" width=\"13.489461358313818\" layout=\"responsive\" alt=\"Mobirise\">\n                        <div placeholder=\"\" class=\"placeholder\"></div>\n                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/mbr.jpg\" alt=\"Mobirise\" title=\"\">\n                    </amp-img>\n                    <div class=\"card-box container\" mbr-class=\"{'align-center': alignTitle == 2, 'align-right': alignTitle == 3}\">\n                        <h2 class=\"mbr-section-subtitle mbr-fonts-style mbr-white\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle, .mbr-section-btn\">Slider Subtitle</h2>\n                        <h1 class=\"mbr-section-title mbr-fonts-style mbr-bold mbr-white\" mbr-class=\"{'mbr-pb-1': showText}\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .mbr-section-btn\">Digital Innovation<br></h1>\n             